    "prettier": "2.8.4",
    "prettier-eslint": "15.0.1",
    "prettier-plugin-tailwindcss": "0.2.3",
    "tailwindcss": "3.1.8",
    "ts-proto": "1.139.0",
    "typescript": "4.9.5",
    "vite": "4.1.4",
