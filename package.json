    "@radix-ui/react-slider": "1.1.0",
    "@radix-ui/react-tabs": "1.0.2",
    "@radix-ui/react-toggle-group": "1.0.2",
    "@react-hook/debounce": "4.0.0",
    "@reduxjs/toolkit": "1.9.3",
    "@tabler/icons-react": "2.5.0",
    "classnames": "2.3.2",
    "fgo-calc": "1.24.2",
    "immer": "9.0.19",
    "protobufjs": "7.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.2",
    "react-redux": "8.0.5",
    "redux": "4.2.1",
    "redux-persist": "6.0.0",
