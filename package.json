{
  "name": "trismegistus",
  "version": "0.0.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "pnpm lint:format && pnpm lint:fix ",
    "type-check": "tsc",
    "proto:generate": "protoc --ts_proto_out=./src/types proto/trismegistus.proto --ts_proto_opt=esModuleInterop=true --ts_proto_opt=env=browser"
  },
